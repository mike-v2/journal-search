{
  "configurations": [
    {
      "name": "Cloud Run: Run/Debug Locally",
      "type": "cloudcode.cloudrun",
      "request": "launch",
      "build": {
        "buildpacks": {
          "path": "package.json",
          "builder": "gcr.io/buildpacks/builder:v1"
        }
      },
      "image": "journal-search",
      "service": {
        "name": "journal-search",
        "containerPort": 8080,
        "resources": {
          "limits": {
            "memory": "256Mi"
          }
        }
      },
      "target": {
        "minikube": {}
      },
      "watch": true
    }
  ]
}